{
  "name": "mata-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:device": "npx expo run:ios --device",
    "dev": "yarn ios",
    "local": "yarn ios",
    "clean:system": "watchman watch-del-all && rm -rf $TMPDIR/metro-cache && npx jest --clearCache && yarn cache clean && rm -rf node_modules && yarn install --frozen-lockfile",
    "prebuild:android": "cross-env EXPO_NO_GIT_STATUS=1 npx expo prebuild --clean --platform android",
    "prebuild::ios": "cross-env EXPO_NO_GIT_STATUS=1 npx expo prebuild --clean --platform ios",
    "clean": "yarn clean:system && yarn prebuild:android && yarn prebuild:android",
    "prebuild": "expo prebuild",
    "expo:upgrade": "yarn add expo@latest && npx expo install --fix",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "lint:errors": "expo lint --fix --quiet",
    "lint:fix-all": "expo lint --fix --max-warnings 0",
    "build:android:local": "cross-env EAS_LOCAL_BUILD_ARTIFACTS_DIR=./build eas build --profile local --platform android --local",
    "build:android:preview": "eas build --profile preview --platform android",
    "build:ios:preview": "eas build --profile preview --platform ios",
    "build:android:preview:local": "cross-env EAS_LOCAL_BUILD_ARTIFACTS_DIR=./build yarn build:android:preview --local",
    "build:apk": "yarn build:android:preview:local",
    "build:release": "cross-env EXPO_APPLE_ID=you@yourdomain.com EAS_LOCAL_BUILD_ARTIFACTS_DIR=./build eas build --profile release --non-interactive",
    "build:release:android": "yarn build:release --platform android",
    "build:release:ios": "yarn build:release --platform ios"
  },
  "dependencies": {
    "@algolia/cache-common": "^4.25.2",
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.1.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-firebase/analytics": "^22.4.0",
    "@react-native-firebase/app": "^22.4.0",
    "@react-native-firebase/auth": "^22.4.0",
    "@react-native-firebase/crashlytics": "^22.4.0",
    "@react-native-firebase/firestore": "^22.4.0",
    "@react-native-firebase/functions": "^22.4.0",
    "@react-native-firebase/messaging": "^22.4.0",
    "@react-native-firebase/remote-config": "^22.4.0",
    "@react-native-firebase/storage": "^22.4.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "algoliasearch": "^5.33.0",
    "date-fns": "^4.1.0",
    "expo": "53.0.20",
    "expo-apple-authentication": "~7.2.4",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "i18next": "^25.3.2",
    "immer": "^10.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.60.0",
    "react-i18next": "^15.6.0",
    "react-native": "0.79.5",
    "react-native-device-info": "^14.0.4",
    "react-native-fbsdk-next": "^13.4.1",
    "react-native-geocoding": "^0.5.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-chat": "^2.8.1",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-maps": "1.20.1",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-popup-menu": "^0.18.0",
    "react-native-reanimated": "~3.17.4",
